<?xml version='1.0' encoding='utf-8'?>
<sumoConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/sumoConfiguration.xsd">

    <input>
        <net-file value="sm_molino_area.net.xml" />
        <route-files value="routes/osm.bicycle.rou.xml,routes/osm.bus.rou.xml,routes/osm.motorcycle.rou.xml,routes/osm.passenger.rou.xml,routes/osm.pedestrian.rou.xml,routes/osm.truck.rou.xml" />
        <additional-files value="sm_molino_area.output_add.xml" />
    </input>

    <output>
        <tripinfo-output value="sm_molino_area.tripinfos.xml" />
        <statistic-output value="sm_molino_area.stats.xml" />
    </output>

    <processing>
        <ignore-route-errors value="true" />
        <tls.actuated.jam-threshold value="30" />
    </processing>

    <routing>
        <device.rerouting.adaptation-steps value="18" />
        <device.rerouting.adaptation-interval value="10" />
    </routing>

    <report>
        <verbose value="true" />
        <duration-log.statistics value="true" />
        <no-step-log value="true" />
    </report>

    <gui_only>
        <gui-settings-file value="sm_molino_area.osm.view.xml" />
    </gui_only>

</sumoConfiguration>