<?xml version='1.0' encoding='utf-8'?>
<sumoConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/sumoConfiguration.xsd">

    <input>
        <net-file value="bayanan_area.net.xml.gz" />
        <route-files value="routes/osm.bus.trips.xml,routes/osm.motorcycle.trips.xml,routes/osm.passenger.trips.xml,routes/osm.truck.trips.xml" />
        <additional-files value="bayanan_area.poly.add.xml,bayanan_area.output_add.xml" />
    </input>

    <output>
        <tripinfo-output value="bayanan_area.tripinfos.xml" />
        <statistic-output value="bayanan_area.stats.xml" />
    <summary-output value="bayanan_area.Bayanan Area_summary.xml" /><emission-output value="bayanan_area.Bayanan Area_emissions.xml" /><edgedata-output value="bayanan_area.Bayanan Area_edgedata.xml" /></output>

    <processing>
        <ignore-route-errors value="true" />
        <tls.actuated.jam-threshold value="30" />
    </processing>

    <routing>
        <device.rerouting.adaptation-steps value="18" />
        <device.rerouting.adaptation-interval value="10" />
    </routing>

    <report>
        <verbose value="true" />
        <duration-log.statistics value="true" />
        <no-step-log value="true" />
    </report>

    <gui_only>
        <gui-settings-file value="bayanan_area.osm_view.xml" />
    </gui_only>

</sumoConfiguration>